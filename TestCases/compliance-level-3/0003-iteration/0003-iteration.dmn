	<semantic:decision id="d_MonthlyPayment" name="MonthlyPayment">
		<semantic:variable name="MonthlyPayment" typeRef="feel:number"/>
		<semantic:informationRequirement>
			<semantic:requiredInput href="#i_Loans"/>
		</semantic:informationRequirement>
		<semantic:knowledgeRequirement>
			<semantic:requiredKnowledge href="#b_PMT2"/>
		</semantic:knowledgeRequirement>
		<semantic:literalExpression>
			<semantic:text>for i in Loans return PMT2(i)</semantic:text>
		</semantic:literalExpression>
	</semantic:decision>
	<semantic:businessKnowledgeModel id="b_PMT2" name="PMT2">
		<semantic:encapsulatedLogic>
			<semantic:formalParameter name="loan" typeRef="tLoan"/>
			<semantic:literalExpression expressionLanguage="FEEL">
				<semantic:text>(loan.amount * loan.rate/12)/(1-(1+loan.rate/12)**-loan.term)</semantic:text>
			</semantic:literalExpression>
		</semantic:encapsulatedLogic>
		<semantic:variable name="PMT2"/>
	</semantic:businessKnowledgeModel>
	<semantic:inputData id="i_Loans" name="Loans">
		<semantic:variable name="Loans" typeRef="tLoan"/>
	</semantic:inputData>
