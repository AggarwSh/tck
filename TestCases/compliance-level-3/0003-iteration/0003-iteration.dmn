	<semantic:itemDefinition isCollection="true" name="tLoan">
		<semantic:itemComponent name="amount">
			<semantic:typeRef>feel:number</semantic:typeRef>
		</semantic:itemComponent>
		<semantic:itemComponent name="rate">
			<semantic:typeRef>feel:number</semantic:typeRef>
